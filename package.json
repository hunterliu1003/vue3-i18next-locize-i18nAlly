{
  "name": "vue-experiment",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "gen:i18next-interface": "i18next-resources-for-ts interface -i ./src/locales/en -o ./src/resources.d.ts",
    "locize-sync": "locize sync --path ./src/locales --project-id 6db84fbf-323e-4089-a4c8-d50e0583a316 --api-key b69c537a-dace-4b21-9fa9-a09145224fc5",
    "locize-sync-all": "locize sync --path ./src/locales --project-id 6db84fbf-323e-4089-a4c8-d50e0583a316 --api-key b69c537a-dace-4b21-9fa9-a09145224fc5 --reference-language-only false",
    "locize-sync-overwrite": "locize sync --path ./src/locales --project-id 6db84fbf-323e-4089-a4c8-d50e0583a316 --api-key b69c537a-dace-4b21-9fa9-a09145224fc5 --reference-language-only false --update-values true",
    "locize-download": "locize download --path ./src/locales --project-id 6db84fbf-323e-4089-a4c8-d50e0583a316 --api-key b69c537a-dace-4b21-9fa9-a09145224fc5"
  },
  "dependencies": {
    "i18next": "^23.5.1",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-chained-backend": "^4.5.0",
    "i18next-http-backend": "^2.2.2",
    "i18next-localstorage-backend": "^4.2.0",
    "i18next-locize-backend": "^6.2.3",
    "i18next-resources-for-ts": "^1.3.3",
    "i18next-resources-to-backend": "^1.1.4",
    "i18next-vue": "^3.0.0",
    "locize": "^2.4.6",
    "locize-cli": "^7.14.11",
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vue-tsc": "^1.8.19"
  }
}
